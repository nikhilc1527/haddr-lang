proc main() {
  let target: i64 = 600851475143;
  let largest: i64 = 1;
  let i: i64 = 1;
  while (i < 775147) {
    if (target % i == 0) {
      let isprime: i64 = 1;
      let j: i64 = 2;
      while (j < 881 && isprime > 0) {
        if (i % j == 0) isprime = 0;
	j = j + 1;
      }
      if (isprime > 0) {
        largest = i;
      }
    }
    i = i + 1;
  }
  printi(largest);
}
