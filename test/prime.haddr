proc isprime(i) {
      let res = 1;
      let j = 2;
      while ((j < i) * res) {
            if (i % j) 0;
            else {
                res = 0;
            }
            j = j + 1;
      }
      res;
}

proc main() {
     let n = 1;
     while(n < 100) {
         let res = isprime(n);
         if (res) printi(n);
         n = n + 1;
     }
}
