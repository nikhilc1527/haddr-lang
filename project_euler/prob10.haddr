PRIME_LENGTH = 2000000;
primes = [PRIME_LENGTH];

N = 0;
while N < PRIME_LENGTH do
    primes[N] = 1;
    N = N + 1;
end;

N = 2;
while N < PRIME_LENGTH do
    if primes[N] do
        j = N * 2;
        while j < PRIME_LENGTH do
            primes[j] = 0;
            j = j + N;
        end;
    else end;
    N = N + 1;
end;

i = 2;
sum = 0;
while i < PRIME_LENGTH do
      if primes[i] do sum = sum + i else end;
      i = i + 1;
end;

puti(sum);

