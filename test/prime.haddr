proc isprime(i: i64) {
      let res: i64 = 1;
      let j: i64 = 2;
      while ((j < i) * res) {
            if (i % j == 0) {
                res = 0;
            }
            j = j + 1;
      }
      return res;
}

proc main() {
     let n: i64 = 1;
     while(n < 100) {
         let res: i64 = isprime(n);
         if (res > 0) printi(n);
         n = n + 1;
     }
}
