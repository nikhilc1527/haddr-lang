func get_num_factors num do
     res = 1;
     i = 2;
     while num > 1 do
          j = 1;
          while num % i == 0 do
                num = num / i;
                j = j + 1;
          end;
          res = res * j;
          i = i + 1;
     end;
     res
end;

i = 1;
n = 0;
MAX_DIV = 50;
divs = get_num_factors(n);
while divs < MAX_DIV do
      n = n + i;
      i = i + 1;
      divs = get_num_factors(n);
      // puti(n); . " : "; puti(divs); . 10;
end;
puti(n);